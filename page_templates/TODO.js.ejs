// src/common/router.js

'/<%- FolderName.toLowerCase() %>': {
    component: dynamicWrapper(app, ['<%- FolderName %>/<%- pageName %>'], () =>
        import ('../routes/<%- FolderName %>/List'))
},
'/<%- FolderName.toLowerCase() %>/<%- pageName %>': {
    component: dynamicWrapper(app, ['<%- FolderName %>/<%- pageName %>'], () =>
        import ('../routes/<%- FolderName %>/<%- pageName %>'))
}