import request from '../../utils/request'
import BASE_URL from '../../config'

// 获取权益定金下拉配置
export async function getSearchConfig (params) {
  return request(BASE_URL + '/api/<%- pageName %>/<%- pageName %>/config', {
    method: 'POST',
    body: params
  })
}

// 获取权益定金订单列表
export async function getTableList (params) {
  return request(BASE_URL + '/api/<%- pageName %>/<%- pageName %>/list', {
    method: 'POST',
    body: params
  })
}
